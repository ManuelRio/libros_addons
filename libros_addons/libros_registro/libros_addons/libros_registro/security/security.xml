<odoo>

    <record id="model_access_data" model="ir.model.access">
        <field name="name">model.access.data</field>
        <field name="model_id" ref="libros_registro.model_library_data"/>
        <field name="group_id" ref="base.group_system"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>

    <record id="model_access_authors" model="ir.model.access">
        <field name="name">model.access.authors</field>
        <field name="model_id" ref="libros_registro.model_library_author"/>
        <field name="group_id" ref="base.group_system"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>
</odoo>